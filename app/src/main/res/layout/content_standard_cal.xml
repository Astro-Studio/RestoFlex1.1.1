<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundpic2"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    app:popupTheme="@style/FullscreenTheme"
    tools:layout_editor_absoluteY="25dp"
    tools:showIn="@layout/activity_standard_cal"><![CDATA[



    app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText1" />













    ]]>

    <EditText
        android:id="@+id/editText1"
        android:layout_width="800dp"
        android:layout_height="69dp"
        android:editable="false"
        android:gravity="end"
        android:textAlignment="textEnd"
        android:textColor="#000000"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="800dp"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        android:editable="false"
        android:font="@font/digital"
        android:fontFamily="@font/digital"
        android:gravity="end"
        android:textAlignment="textEnd"
        android:textColor="#FFFFFF"
        android:textSize="150sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText1" />

    <Button
        android:id="@+id/history"
        android:layout_width="400dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/history"
        android:textColor="@drawable/button_text_color2"
        android:textSize="40dp"
        app:layout_constraintStart_toEndOf="@+id/backSpace"
        app:layout_constraintTop_toBottomOf="@+id/editText2" />

    <Button
        android:id="@+id/num4"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="4"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num7" />

    <Button
        android:id="@+id/dot"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="."
        android:textColor="@drawable/button_text_color2"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/num0"
        app:layout_constraintTop_toBottomOf="@+id/num3" />

    <Button
        android:id="@+id/num3"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="3"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num2"
        app:layout_constraintTop_toBottomOf="@+id/num6" />

    <Button
        android:id="@+id/num0"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="0"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/posneg"
        app:layout_constraintTop_toBottomOf="@+id/num2" />

    <Button
        android:id="@+id/sqrt"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/sqrt"
        android:textColor="@drawable/button_text_color2"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/square"
        app:layout_constraintTop_toBottomOf="@+id/multiply" />

    <Button
        android:id="@+id/square"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/square"
        android:textColor="@drawable/button_text_color2"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/closeBracket"
        app:layout_constraintTop_toBottomOf="@+id/dot" />

    <Button
        android:id="@+id/divide"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:foreground="#22065CB8"
        android:onClick="onClick"
        android:text="@string/divide"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num3"
        app:layout_constraintTop_toBottomOf="@+id/minus" />

    <Button
        android:id="@+id/plus"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:foreground="#22065CB8"
        android:onClick="onClick"
        android:text="+"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num9"
        app:layout_constraintTop_toBottomOf="@+id/history" />

    <Button
        android:id="@+id/posneg"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/posneg"
        android:textColor="@drawable/button_text_color2"
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num1" />

    <Button
        android:id="@+id/num6"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="6"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num5"
        app:layout_constraintTop_toBottomOf="@+id/num9" />

    <Button
        android:id="@+id/num2"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="2"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num1"
        app:layout_constraintTop_toBottomOf="@+id/num5" />

    <Button
        android:id="@+id/num1"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="1"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num4" />

    <Button
        android:id="@+id/num9"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="9"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num8"
        app:layout_constraintTop_toBottomOf="@+id/history" />

    <Button
        android:id="@+id/openBracket"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/openBracket"
        android:textColor="@drawable/button_text_color2"
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/posneg" />

    <Button
        android:id="@+id/closeBracket"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/closeBracket"
        android:textColor="@drawable/button_text_color2"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/openBracket"
        app:layout_constraintTop_toBottomOf="@+id/num0" />

    <Button
        android:id="@+id/minus"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:foreground="#22065CB8"
        android:onClick="onClick"
        android:text="-"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num6"
        app:layout_constraintTop_toBottomOf="@+id/plus" />

    <Button
        android:id="@+id/num7"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="7"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clear" />

    <Button
        android:id="@+id/num5"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="5"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num4"
        app:layout_constraintTop_toBottomOf="@+id/num8" />

    <Button
        android:id="@+id/equal"
        android:layout_width="800dp"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:foreground="#22065CB8"
        android:onClick="onClick"
        android:text="="
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/closeBracket" />

    <Button
        android:id="@+id/backSpace"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/backspace"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/clear"
        app:layout_constraintTop_toBottomOf="@+id/editText2" />

    <Button
        android:id="@+id/multiply"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:foreground="#22065CB8"
        android:onClick="onClick"
        android:text="x"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/dot"
        app:layout_constraintTop_toBottomOf="@+id/divide" />

    <Button
        android:id="@+id/clear"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="@string/clear"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText2" />

    <Button
        android:id="@+id/num8"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="@drawable/button_background2"
        android:editable="false"
        android:onClick="onClick"
        android:text="8"
        android:textColor="@drawable/button_text_color2"
        android:textSize="80dp"
        app:layout_constraintStart_toEndOf="@+id/num7"
        app:layout_constraintTop_toBottomOf="@+id/backSpace" />

</android.support.constraint.ConstraintLayout>
